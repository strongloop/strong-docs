{
  "name": "strong-docs",
  "description": "Sleek, intuitive, and powerful documentation site generator",
  "version": "4.2.1",
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/strong-docs.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --outdir lib",
    "prepare": "npm run snyk-protect && npm run build",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "test": "mocha --require source-map-support/register --timeout 60000",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dox": "^0.9.0",
    "ejs": "^3.0.1",
    "express": "^4.16.3",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.2",
    "highlight.js": "^9.17.1",
    "markdown": "^0.5.0",
    "marked": "^0.8.0",
    "optimist": "^0.6.0",
    "strong-task-emitter": "^0.0.8",
    "typedoc": "^0.16.2",
    "underscore.string": "^3.3.4",
    "snyk": "^1.362.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.0",
    "@types/glob": "^7.1.1",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.7.2",
    "@types/node": "^10.17.13",
    "@types/underscore.string": "0.0.38",
    "chai": "^4.1.0",
    "mocha": "^7.0.0",
    "prettier": "^1.19.1",
    "source-map-support": "^0.5.16",
    "tslint": "^5.20.1",
    "typescript": "~3.7.4"
  },
  "bin": {
    "sdocs": "./bin/cli.js"
  },
  "license": "MIT",
  "author": "IBM Corp.",
  "snyk": true
}
