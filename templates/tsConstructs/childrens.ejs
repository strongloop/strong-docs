<%   
    var properties = [];
    var variables = [];
    if(locals.children && locals.children.length > 0){
        children.forEach(function(child){
            if(child.kindString === 'Property' && child.shouldDocument){
                properties.push(child);
            }
            if(child.kindString === 'Variable' && child.shouldDocument){
                variables.push(child);
            }
        })
        if(properties && properties.length > 0){
%>
            <%- include('properties', {'properties': properties}); %>
<%
        }
        if(variables && variables.length > 0){
%>
            <%- include('variables', {'variables': variables}); %>
<% 
        }
        children.forEach(function(child){
            if(child.kindString === 'Constructor' && child.shouldDocument){%>
                 <%- include('constructor', child); %>
            <%}
            if(child.kindString === 'Method' && child.shouldDocument){%>
                <%- include('function', child); %>
            <%}
        })
    }
%>